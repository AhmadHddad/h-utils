{
  "version": "0.2.4",
  "license": "MIT",
  "main": "lib/index.js",
  "description": "A handy utils for modern JS developers",
  "module": "lib/index.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir lib",
    "build": "yarn build:cjs && yarn build:es && yarn run fix",
    "fix": "fix-esm-import-path lib/index.js",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "npx typedoc src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hd-utils",
  "author": "AhmadHddad",
  "keywords": [
    "utils",
    "@h/utils",
    "hddad-utils",
    "@h",
    "hd-utils",
    "hUtils",
    "hd",
    "hdutils",
    "hdUtils",
    "h-utils",
    "hd-utils",
    "js",
    "stringUtils",
    "arrayUtils",
    "Mui",
    "AhmadHddad",
    "Hddad",
    "H",
    "map",
    "lodash"
  ],
  "repository": "https://github.com/AhmadHddad/h-utils",
  "bugs": "https://github.com/AhmadHddad/h-utils/issues",
  "size-limit": [
    {
      "path": "dist/h-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/h-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "agadoo": "^3.0.0",
    "husky": "^8.0.1",
    "fix-esm-import-path": "^1.4.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.25",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "hd-intersection-observer": "^0.1.0"
  }
}
